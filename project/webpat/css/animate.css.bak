/* slidein */
@-webkit-keyframes slidein {
	
  0% {
  	min-height: 0;        
  }

    
  100% {
		min-height: 525px;
		z-index: 1;  
  }
  
} 

@keyframes slidein {
	
  from {
		min-height: 0;
  }

  to {
		min-height: 525px;
		z-index: 1;	
  }
}

/* disappear */
@-webkit-keyframes disappear {
	
  from { 
	 	font-size: 30px;
		margin: 1em auto;
		top: 0;
		opacity: 1; 	
  }

  to {  	
		font-size: 0;
		margin: 0;
		top: -100px;
		opacity: 0;
		visibility: hidden;	  	
  }
  
} 

@keyframes disappear {
	
  from { 
	 	font-size: 30px;
		margin: 1em auto;
		top: 0;
		opacity: 1; 	
  }

  to {  	
		font-size: 0;
		margin: 0;
		top: -100px;
		opacity: 0;
		visibility: hidden;	  	
  }
  
}


/* tabs animations */
.tabs-anim-slide-up > .tab-content ul  > li {
	-webkit-transform: translateY(-15%);	
     -moz-transform: translateY(-15%);	
      -ms-transform: translateY(-15%);	
       -o-transform: translateY(-15%);
          transform: translateY(-15%);	
}

.tabs-anim-slide-right > .tab-content ul  > li {
	
	-webkit-transform: translateX(15%);
     -moz-transform: translateX(15%);
      -ms-transform: translateX(15%);
       -o-transform: translateX(15%);
          transform: translateX(15%);	
}

.tabs-anim-slide-down > .tab-content ul  > li {
	
	-webkit-transform: translateY(15%);
     -moz-transform: translateY(15%);
      -ms-transform: translateY(15%);
       -o-transform: translateY(15%);
          transform: translateY(15%);		
}

.tabs-anim-slide-left > .tab-content ul  > li {
	
	-webkit-transform: translateX(-15%);
     -moz-transform: translateX(-15%);
      -ms-transform: translateX(-15%);
       -o-transform: translateX(-15%);
          transform: translateX(-15%);
}

.tabs-anim-slide-up-left > .tab-content ul  > li {
	
	-webkit-transform: translate(-15%,-15%);
     -moz-transform: translate(-15%,-15%);
      -ms-transform: translate(-15%,-15%);
       -o-transform: translate(-15%,-15%);
          transform: translate(-15%,-15%);
}
.tabs-anim-slide-up-right > .tab-content ul  > li {
	
	-webkit-transform: translate(15%,-15%);	
     -moz-transform: translate(15%,-15%);	
      -ms-transform: translate(15%,-15%);	
       -o-transform: translate(15%,-15%);	
          transform: translate(15%,-15%);	
}
.tabs-anim-slide-down-right > .tab-content ul  > li {
	
	-webkit-transform: translate(15%,15%);	
     -moz-transform: translate(15%,15%);	
      -ms-transform: translate(15%,15%);	
       -o-transform: translate(15%,15%);	
          transform: translate(15%,15%);	
}
.tabs-anim-slide-down-left > .tab-content ul  > li {
	
	-webkit-transform: translate(-15%,15%);		
     -moz-transform: translate(-15%,15%);	
      -ms-transform: translate(-15%,15%);	
       -o-transform: translate(-15%,15%);	
          transform: translate(-15%,15%);	
}

.tabs  > .tab-content-1:checked ~ .tab-content ul > .tab-content-1,
.tabs  > .tab-content-2:checked ~ .tab-content ul > .tab-content-2,
.tabs  > .tab-content-3:checked ~ .tab-content ul > .tab-content-3,
.tabs  > .tab-content-4:checked ~ .tab-content ul > .tab-content-4,
.tabs  > .tab-content-5:checked ~ .tab-content ul > .tab-content-5,
.tabs  > .tab-content-6:checked ~ .tab-content ul > .tab-content-6,
.tabs  > .tab-content-7:checked ~ .tab-content ul > .tab-content-7{
	
	-webkit-transform: translate(0,0);
     -moz-transform: translate(0,0);
      -ms-transform: translate(0,0);
       -o-transform: translate(0,0);
          transform: translate(0,0);	
}

.tabs-anim-scale > .tab-content ul  > li {
	
	-webkit-transform: scale(0.1,0.1);
     -moz-transform: scale(0.1,0.1);
      -ms-transform: scale(0.1,0.1);
       -o-transform: scale(0.1,0.1);
          transform: scale(0.1,0.1);
}
.tabs-anim-scale > .tab-content-1:checked ~ .tab-content ul > .tab-content-1,
.tabs-anim-scale > .tab-content-2:checked ~ .tab-content ul > .tab-content-2,
.tabs-anim-scale > .tab-content-3:checked ~ .tab-content ul > .tab-content-3,
.tabs-anim-scale > .tab-content-4:checked ~ .tab-content ul > .tab-content-4,
.tabs-anim-scale > .tab-content-5:checked ~ .tab-content ul > .tab-content-5,
.tabs-anim-scale > .tab-content-6:checked ~ .tab-content ul > .tab-content-6,
.tabs-anim-scale > .tab-content-7:checked ~ .tab-content ul > .tab-content-7{
	
	-webkit-transform: scale(1,1);
     -moz-transform: scale(1,1);
      -ms-transform: scale(1,1);
       -o-transform: scale(1,1);
          transform: scale(1,1);
}

.tabs-anim-rotate > .tab-content ul  > li {
	
	-webkit-transform: rotate(90deg);
     -moz-transform: rotate(90deg);
      -ms-transform: rotate(90deg);
       -o-transform: rotate(90deg);
          transform: rotate(90deg);
}
.tabs-anim-rotate.sky-tabs-pos-right > .tab-content ul  > li,
.tabs-anim-rotate.sky-tabs-pos-top-right > .tab-content ul  > li {
	
	-webkit-transform: rotate(-90deg);
     -moz-transform: rotate(-90deg);
      -ms-transform: rotate(-90deg);
       -o-transform: rotate(-90deg);
          transform: rotate(-90deg);
}
.tabs-anim-rotate > .tab-content-1:checked ~ .tab-content ul > .tab-content-1,
.tabs-anim-rotate > .tab-content-2:checked ~ .tab-content ul > .tab-content-2,
.tabs-anim-rotate > .tab-content-3:checked ~ .tab-content ul > .tab-content-3,
.tabs-anim-rotate > .tab-content-4:checked ~ .tab-content ul > .tab-content-4,
.tabs-anim-rotate > .tab-content-5:checked ~ .tab-content ul > .tab-content-5,
.tabs-anim-rotate > .tab-content-6:checked ~ .tab-content ul > .tab-content-6,
.tabs-anim-rotate > .tab-content-7:checked ~ .tab-content ul > .tab-content-7{
	
	-webkit-transform: rotate(0deg);
     -moz-transform: rotate(0deg);
      -ms-transform: rotate(0deg);
       -o-transform: rotate(0deg);
          transform: rotate(0deg);	
}

.tabs-anim-flip > .tab-content ul  {
	perspective: 2000px;
	-o-perspective: 2000px;
	-ms-perspective: 2000px;
	-moz-perspective: 2000px;
	-webkit-perspective: 2000px;
	
	perspective-origin: 50% 50%;
	-o-perspective-origin: 50% 50%;
	-ms-perspective-origin: 50% 50%;
	-moz-perspective-origin: 50% 50%;
	-webkit-perspective-origin: 50% 50%;
	
}
.tabs-anim-flip > .tab-content ul  > li {
	
	-webkit-transform: rotateX(-90deg);
     -moz-transform: rotateX(-90deg);
      -ms-transform: rotateX(-90deg);
       -o-transform: rotateX(-90deg);
          transform: rotateX(90deg);		
}
.tabs-anim-flip.sky-tabs-pos-left > .tab-content ul  > li {
	
	-webkit-transform: rotateY(90deg);
     -moz-transform: rotateY(90deg);
      -ms-transform: rotateY(90deg);
       -o-transform: rotateY(90deg);
          transform: rotateY(90deg);			
}
.tabs-anim-flip.sky-tabs-pos-right > .tab-content ul  > li {
	
	-webkit-transform: rotateY(-90deg);
     -moz-transform: rotateY(-90deg);
      -ms-transform: rotateY(-90deg);
       -o-transform: rotateY(-90deg);
          transform: rotateY(-90deg);		
}
.tabs-anim-flip > .tab-content-1:checked ~ .tab-content ul  > .tab-content-1,
.tabs-anim-flip > .tab-content-2:checked ~ .tab-content ul  > .tab-content-2,
.tabs-anim-flip > .tab-content-3:checked ~ .tab-content ul  > .tab-content-3,
.tabs-anim-flip > .tab-content-4:checked ~ .tab-content ul  > .tab-content-4,
.tabs-anim-flip > .tab-content-5:checked ~ .tab-content ul  > .tab-content-5,
.tabs-anim-flip > .tab-content-6:checked ~ .tab-content ul  > .tab-content-6,
.tabs-anim-flip > .tab-content-7:checked ~ .tab-content ul  > .tab-content-7{
	
	-webkit-transform: rotateX(0deg);
     -moz-transform: rotateX(0deg);
      -ms-transform: rotateX(0deg);
       -o-transform: rotateX(0deg);
          transform: rotateX(0deg);	

	-webkit-transform-delay: 0.2s;
     -moz-transform-delay: 0.2s;
      -ms-transform-delay: 0.2s;
       -o-transform-delay: 0.2s;
          transform-delay: 0.2s;

}
