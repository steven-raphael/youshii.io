}
 
a,button,input {
  color: #000;
	text-decoration: none;	
	outline:none;
	blr: expression(this.onFocus=this.blur());	
}

a:hover {
	color: #edecec;
	text-decoration: none;
	outline:none;
	blr: expression(this.onFocus=this.blur());	
	
	-webkit-transition: all .3s;
	   -moz-transition: all .3s;
	     -o-transition: all .3s;
	        transition: all .3s;	
}

