}

.tabs {
	position: absolute;
	top: 55px;
	width: 100%;		
	text-align: center;	
	z-index: 100;
}

.tabs > input {
	position: absolute;
	display: none;
}

.tabs  > label{
	position: relative;
	display: inline-block;
	padding: 5px;
	font-size: 16px;
	margin: auto 5px;	
	cursor: pointer;
	border-radius: 5px;
	
	-webkit-box-shadow: 0 2px 7px -2px transparent;
	   -moz-box-shadow: 0 2px 7px -2px transparent;
	        box-shadow: 0 2px 7px -2px transparent;	
	          
  -webkit-transition: all .4s;	
	   -moz-transition: all .4s;	
	   	-ms-transition: all .4s;	
			 -o-transition: all .4s;	
	        transition: all .4s;	

	z-index: 1;
			
}

.tabs >  input:checked + label {
	cursor: default;
	background: #9f9f9f;	
	color: #edecec;	
	
	-webkit-box-shadow: 0 2px 7px -2px #000;
	   -moz-box-shadow: 0 2px 7px -2px #000;
	        box-shadow: 0 2px 7px -2px #000;
}

.tab-content > ul{
	position: relative;
	display: block;
	margin: 0 auto;
	width: 600px;
	left: 6em;
}

.tab-content  > ul > li {
	position: absolute;
	background: rgb(159,159,159); /* Old browsers */
	background: -moz-linear-gradient(top,  rgba(159,159,159,1) 0%, rgba(201,201,202,1) 100%); 
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(159,159,159,1)), color-stop(100%,rgba(201,201,202,1))); 
	background: -webkit-linear-gradient(top,  rgba(159,159,159,1) 0%,rgba(201,201,202,1) 100%);
	background: -o-linear-gradient(top,  rgba(159,159,159,1) 0%,rgba(201,201,202,1) 100%);
	background: -ms-linear-gradient(top,  rgba(159,159,159,1) 0%,rgba(201,201,202,1) 100%); 
	background: linear-gradient(to bottom,  rgba(159,159,159,1) 0%,rgba(201,201,202,1) 100%); 
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#9f9f9f', endColorstr='#c9c9ca',GradientType=0 );

	top: 5px;
	width: 476px;
	
	padding: 20px 25px 0;	
	border-radius: 5px 5px 0 5px;
	opacity: 0;
	
	-webkit-box-shadow: 0 2px 7px -2px #000;
	   -moz-box-shadow: 0 2px 7px -2px #000;
	        box-shadow: 0 2px 7px -2px #000;	
	        	
	-webkit-transform-origin: 0% 0%;	
	   -moz-transform-origin: 0% 0%;
			-ms-transform-origin: 0% 0%;
			 -o-transform-origin: 0% 0%;
					transform-origin: 0% 0%;
					
	-webkit-transition: opacity 0.8s, -webkit-transform 0.8s;
	   -moz-transition: opacity 0.8s,    -moz-transform 0.8s;		
	    -ms-transition: opacity 0.8s,     -ms-transform 0.8s;		
	     -o-transition: opacity 0.8s,      -o-transform 0.8s;	
          transition: opacity 0.8s,         transform 0.8s;	

	
}

.tabs  > .tab-content-1:checked ~ .tab-content ul > .tab-content-1,
.tabs  > .tab-content-2:checked ~ .tab-content ul > .tab-content-2,
.tabs  > .tab-content-3:checked ~ .tab-content ul > .tab-content-3,
.tabs  > .tab-content-4:checked ~ .tab-content ul > .tab-content-4,
.tabs  > .tab-content-5:checked ~ .tab-content ul > .tab-content-5,
.tabs  > .tab-content-6:checked ~ .tab-content ul > .tab-content-6,
.tabs  > .tab-content-7:checked ~ .tab-content ul > .tab-content-7{
	position: relative;
	z-index: 1;
	opacity: 1;
}

