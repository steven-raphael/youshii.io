/* Section */
#content{
	width: 100%;	
	margin:0;
	padding:0;
}

.jms-slideshow{
	position: relative;	
	max-width: 100%;
	width: 100%;
	overflow: hidden;		
}

.jms-wrapper {
	width: auto;
	min-width: 600px;	
	outline: none;
		
  -webkit-animation: slidein .8s cubic-bezier(0.420, 0.000, 0.580, 1.000) 2s 1 both normal running; 
          animation: slidein .8s cubic-bezier(0.420, 0.000, 0.580, 1.000) 2s 1 both normal running; 
          	
	-webkit-transition: all 2s linear;
	   -moz-transition: all 2s linear;
	    -ms-transition: all 2s linear;
	     -o-transition: all 2s linear;
	        transition: all 2s linear;
	
}

.color-1 {
	background: url(../image/bg-1.png) no-repeat center center;
	background-size: cover;
}

.color-2 {
	background: url(../image/bg-2.jpg) no-repeat center center;
	background-size: cover;
}

.color-3 {
	background: url(../image/bg-4.jpg) no-repeat center center;
	background-size: cover;
}

.step {	
	width: 1000px;
  height: 420px;
	display: block;
	
	-webkit-transition: all 2s linear;
	   -moz-transition: all 2s linear;
	    -ms-transition: all 2s linear;
	     -o-transition: all 2s linear;
	        transition: all 2s linear;
}

.step:not(.active) {
	opacity: 0;
	filter: alpha(opacity=0); /* internet explorer */
    -ms-filter: "progid:DXImageTransform.Microsoft.Alpha(opacity=0)"; /*IE8*/
}

.jms-content{
	margin: 0px 370px 0px 20px;
	position: relative;
	clear: both;
}

.jms-content h3{
	color: #fff;
	font-size: 52px;
	font-weight: bold;
	text-shadow: 1px 1px 1px rgba(0,0,0,0.1);
	margin-left: 0;
	padding: 60px 0 10px 0;
	opacity: 1;
	
	-webkit-transition: all 0.4s ease-in-out 1s;
	   -moz-transition: all 0.4s ease-in-out 1s;
	    -ms-transition: all 0.4s ease-in-out 1s;
	     -o-transition: all 0.4s ease-in-out 1s;
	        transition: all 0.4s ease-in-out 1s;		
}

.step:not(.active) .jms-content h3{
	opacity: 0;
	margin-left: -18em;
}


.jms-content p {
	color: #fff;
	text-shadow: 1px 1px 1px rgba(0,0,0,0.1);
	font-size: 25px;
	font-weight: normal;
	position: relative;
	left: 0;
	opacity: 1;	
	line-height: 1.5em;
	
	-webkit-transition: all 0.4s ease-in-out 1.2s;
	   -moz-transition: all 0.4s ease-in-out 1.2s;
	    -ms-transition: all 0.4s ease-in-out 1.2s;
	     -o-transition: all 0.4s ease-in-out 1.2s;
	        transition: all 0.4s ease-in-out 1.2s;		
}

.step:not(.active) .jms-content p{
	opacity: 0;
	left: -16em;
}

a.jms-link{
	background: #f0f0f0;
	display: inline-block;
	clear: both;
	text-transform: uppercase;
	padding: 8px 15px;
	margin-top: 40px;
	font-size: 16px;
	font-weight: bold;
	color: #000;
	border-radius: 4px;
	text-shadow: 1px 1px 1px rgba(0, 0, 0, 0.3);
	box-shadow: 1px 1px 1px rgba(0, 0, 0, 0.5);
	opacity: 1;

	
	-webkit-transition: all 0.4s ease-in-out 1.5s;
	   -moz-transition: all 0.4s ease-in-out 1.5s;
	    -ms-transition: all 0.4s ease-in-out 1.5s;
	     -o-transition: all 0.4s ease-in-out 1.5s;
	        transition: all 0.4s ease-in-out 1.5s;
}

.step:not(.active) a.jms-link{
	opacity: 0;
	margin-top: 80px;
}

.step img.jms-img{
	position: absolute;
	right: -9em;
	top: 3em;
	opacity: 1;
	
	-webkit-transform: scale(1.8);
	   -moz-transform: scale(1.8);
	     -o-transform: scale(1.8);
	        transform: scale(1.8);
	        	
	-webkit-transition: all 0.4s ease-in-out 1s;
	   -moz-transition: all 0.4s ease-in-out 1s;
	    -ms-transition: all 0.4s ease-in-out 1s;
	     -o-transition: all 0.4s ease-in-out 1s;
	        transition: all 0.4s ease-in-out 1s;	
}

.step:not(.active) img.jms-img{
	right: -26em;
	opacity: 0;	
}

.jms-dots{
	width: 100%;
	position: absolute;
	text-align: center;

	left: 0px;
	bottom: 20px;
	z-index: 2000;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	-o-user-select: none;
	user-select: none;
}

.jms-dots span{
	display: inline-block;
	position: relative;
	width: 12px;
	height: 12px;
	border-radius: 50%;
	background: #777;
	margin: 3px;
	cursor: pointer;
	box-shadow: 
		1px 1px 1px rgba(0,0,0,0.1) inset, 
		1px 1px 1px rgba(255,255,255,0.3);
}

.jms-dots span.jms-dots-current:after{
	content: '';
	background: #ffffff; /* Old browsers */
	background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#ffffff), color-stop(47%,#f6f6f6), color-stop(100%,#ededed)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(top, #ffffff 0%,#f6f6f6 47%,#ededed 100%); /* Chrome10+,Safari5.1+ */
	background:     -ms-linear-gradient(top, #ffffff 0%,#f6f6f6 47%,#ededed 100%); /* IE10+ */
	background:    -moz-linear-gradient(top, #ffffff 0%, #f6f6f6 47%, #ededed 100%); /* FF3.6+ */
	background:      -o-linear-gradient(top, #ffffff 0%,#f6f6f6 47%,#ededed 100%); /* Opera 11.10+ */
	background:         linear-gradient(top, #ffffff 0%,#f6f6f6 47%,#ededed 100%); /* W3C */
	filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#ededed',GradientType=0 ); /* IE6-9 */	
	
	width: 8px;
	height: 8px;
	position: absolute;
	top: 2px;
	left: 2px;
	border-radius: 50%;
}

.jms-slideshow:hover .jms-arrows span{
	top: 50%;	
	opacity: 1;
	visibility: visible;		
	

	-webkit-transition: all .3s;
	   -moz-transition: all .3s;
	     -o-transition: all .3s;
	        transition: all .3s;		
}

.jms-arrows{	
	-webkit-user-select: none;
	   -moz-user-select: none;
	    -ms-user-select: none;
	     -o-user-select: none;
	        user-select: none;        
}

.jms-arrows span{
	position: absolute;
	top: 47%;
	opacity: 0;
	visibility: hidden;		
	margin-top: -40px;
	height: 80px;
	width: 30px;
	cursor: pointer;
	z-index: 2000;
	box-shadow: 1px 1px 4px rgba(0, 0, 0, 0.1);
	

	-webkit-transition: all .3s;
	   -moz-transition: all .3s;
	     -o-transition: all .3s;
	        transition: all .3s;			
}

.jms-arrows span.jms-arrows-prev{
	background: #fff;
	left: 10px;
	border-radius: 510px;
	width: 40px;
	height: 40px;
}

.jms-arrows span.jms-arrows-prev::before{
	content: "";
	position: absolute;
	display: block;
	background: #bbbbba;
	height: 15px;
	width: 2px;
	left: 18px;
	top: 9px;
	
	-webkit-transform:rotate(55deg);
	   -moz-transform:rotate(55deg);
	     -o-transform:rotate(55deg);
	        transform:rotate(55deg);
}

.jms-arrows span.jms-arrows-prev::after{
	content: "";
	position: absolute;
	display: block;
	background: #bbbbba;
	height: 15px;
	width: 2px;
	left: 18px;
	top: 17px;
	
	-webkit-transform:rotate(-55deg);
	   -moz-transform:rotate(-55deg);
	     -o-transform:rotate(-55deg);
	        transform:rotate(-55deg);
}

.jms-arrows span.jms-arrows-prev:hover::before,
.jms-arrows span.jms-arrows-prev:hover::after{	
	
	left: 15px;		
	-webkit-transition: all .3s;
	   -moz-transition: all .3s;
	     -o-transition: all .3s;
	        transition: all .3s;			
}

.jms-arrows span.jms-arrows-next{
	background: #fff;
	right: 10px;
	border-radius: 510px;
	width: 40px;
	height: 40px;
	
	-webkit-transform: rotate(180deg);
	   -moz-transform: rotate(180deg);
	     -o-transform: rotate(180deg);
	        transform: rotate(180deg);
}

.jms-arrows span.jms-arrows-next::before{
	content: "";
	position: absolute;
	display: block;
	background: #bbbbba;
	height: 15px;
	width: 2px;
	left: 18px;
	top: 9px;
	
	-webkit-transform: rotate(55deg);
	   -moz-transform: rotate(55deg);
	     -o-transform: rotate(55deg);
	        transform: rotate(55deg);
}

.jms-arrows span.jms-arrows-next::after{
	content: "";
	position: absolute;
	display: block;
	background: #bbbbba;
	height: 15px;
	width: 2px;
	left: 18px;
	top: 17px;
	
	-webkit-transform: rotate(-55deg);
	   -moz-transform: rotate(-55deg);
	     -o-transform: rotate(-55deg);
	        transform: rotate(-55deg);
}

.jms-arrows span.jms-arrows-next:hover::before,
.jms-arrows span.jms-arrows-next:hover::after{	
	
	left: 15px;		
	-webkit-transition: all .3s;
	   -moz-transition: all .3s;
	     -o-transition: all .3s;
	        transition: all .3s;			
}

/* Not supported style */
